<div class="sidebar">
  <% if (index == true) { %>
    <div class="sidebar-inner-index">
        <ul class="main-menu">
          <%- partial('partials/main_menu', { context: 'sidebar' }) %>
        </ul>
      </div>
  <% } else { %>
    <div class="sidebar-inner">
      <ul class="main-menu">
        <br><h2><a href="<%- url_for("/") %>"><%- config.title %></a></h2>
        <%- partial('partials/main_menu', { context: 'sidebar' }) %>
      </ul>
      <% if (type && type !== 'search') { %>
        <div class="list">
          <h2>
            <% titles = {
              api: 'API',
              examples: '示例',
              guide: '教程',
              cookbook: 'Cookbook<sup class="beta">beta</sup>',
              'style-guide': '风格指南'
            } %>
            <%- type %>
          </h2>
          <%- partial('partials/toc', { type: type }) %>
        </div>
      <% } %>
      <% if (page.link) { %>
        <div class="footer">
          <% if (page.link.repo) { %>
            <a class="button" href="<%- page.link.repo %>"><i class="fab fa-github fa-fw"></i> 源码</a>
          <% } %>
          <% if (page.link.download) { %>
            <a class="button" href="<%- page.link.download %>"><i class="fas fa-download fa-fw"></i> 下载</a>
          <% } %>
        </div>
      <% } %>
    </div>
  <% } %>
</div>
